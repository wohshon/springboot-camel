{
  "apiVersion" : "v1",
  "kind" : "List",
  "items" : [ {
    "apiVersion" : "v1",
    "kind" : "Service",
    "metadata" : {
      "annotations" : {
        "fabric8.io/iconUrl" : "img/icons/camel.svg",
        "prometheus.io/port" : "9779",
        "prometheus.io/scrape" : "true"
      },
      "labels" : {
        "expose" : "true",
        "app" : "product",
        "provider" : "fabric8",
        "version" : "1.0",
        "group" : "com.redhat"
      },
      "name" : "product"
    },
    "spec" : {
      "ports" : [ {
        "name" : "http",
        "port" : 8080,
        "protocol" : "TCP",
        "targetPort" : 8080
      } ],
      "selector" : {
        "app" : "product",
        "provider" : "fabric8",
        "group" : "com.redhat"
      }
    }
  }, {
    "apiVersion" : "extensions/v1beta1",
    "kind" : "Deployment",
    "metadata" : {
      "annotations" : {
        "fabric8.io/iconUrl" : "img/icons/camel.svg",
        "fabric8.io/metrics-path" : "dashboard/file/camel-routes.json/?var-project=product&var-version=1.0"
      },
      "labels" : {
        "app" : "product",
        "provider" : "fabric8",
        "version" : "1.0",
        "group" : "com.redhat"
      },
      "name" : "product"
    },
    "spec" : {
      "replicas" : 1,
      "revisionHistoryLimit" : 2,
      "selector" : {
        "matchLabels" : {
          "app" : "product",
          "provider" : "fabric8",
          "group" : "com.redhat"
        }
      },
      "template" : {
        "metadata" : {
          "annotations" : {
            "fabric8.io/iconUrl" : "img/icons/camel.svg",
            "fabric8.io/metrics-path" : "dashboard/file/camel-routes.json/?var-project=product&var-version=1.0"
          },
          "labels" : {
            "app" : "product",
            "provider" : "fabric8",
            "version" : "1.0",
            "group" : "com.redhat"
          }
        },
        "spec" : {
          "containers" : [ {
            "env" : [ {
              "name" : "SPRING_APPLICATION_JSON",
              "value" : "{\"server\":{\"undertow\":{\"io-threads\":1, \"worker-threads\":2 }}}"
            }, {
              "name" : "KUBERNETES_NAMESPACE",
              "valueFrom" : {
                "fieldRef" : {
                  "fieldPath" : "metadata.namespace"
                }
              }
            } ],
            "image" : "product:1.0",
            "imagePullPolicy" : "IfNotPresent",
            "livenessProbe" : {
              "httpGet" : {
                "path" : "/health",
                "port" : 8081,
                "scheme" : "HTTP"
              },
              "initialDelaySeconds" : 180
            },
            "name" : "spring-boot",
            "ports" : [ {
              "containerPort" : 8080,
              "name" : "http",
              "protocol" : "TCP"
            }, {
              "containerPort" : 9779,
              "name" : "prometheus",
              "protocol" : "TCP"
            }, {
              "containerPort" : 8778,
              "name" : "jolokia",
              "protocol" : "TCP"
            } ],
            "readinessProbe" : {
              "httpGet" : {
                "path" : "/health",
                "port" : 8081,
                "scheme" : "HTTP"
              },
              "initialDelaySeconds" : 10
            },
            "resources" : {
              "limits" : {
                "cpu" : "1.0",
                "memory" : "256Mi"
              },
              "requests" : {
                "cpu" : "0.2",
                "memory" : "256Mi"
              }
            },
            "securityContext" : {
              "privileged" : false
            }
          } ]
        }
      }
    }
  } ]
}